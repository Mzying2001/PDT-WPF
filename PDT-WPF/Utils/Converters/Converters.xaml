<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:PDT_WPF.Utils.Converters">

    <!--使HandyControl的流布局自适应组数-->
    <!--<local:HcWaterfallGroupsAdapter
        x:Key="GroupsAdapter"
        ItemWidth="300"/>-->

    <!--格式化日期-->
    <local:DateFormater
        x:Key="DateFormater"/>

    <!--网址前补充https-->
    <local:HttpsUrlFormater
        x:Key="HttpsUrlFormater"/>

    <!--枚举转整数-->
    <local:EnumToInt
        x:Key="EnumToInt"/>

    <!--获取轮播图跳转类型名称-->
    <local:BoardPhotoJumpTypeToString
        x:Key="BoardPhotoJumpTypeToString"/>

    <!--字符串不为空-->
    <local:StringNotEmpty
        x:Key="StringNotEmpty"/>

    <!--整数不为负数-->
    <local:IntNotNegative
        x:Key="IntNotNegative"/>

    <!--非负时可见-->
    <local:VisiableIfNotNegative
        x:Key="VisiableIfNotNegative"/>

    <!--判断多个字符串不为空-->
    <local:StringArrNotAnyEmpty
        x:Key="StringArrNotAnyEmpty"/>

</ResourceDictionary>